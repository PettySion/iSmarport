<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.szip.sportwatch">

    <uses-permission android:name="android.permission.BODY_SENSORS" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.NETWORK_PROVIDER" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.READ_SMS" />
    <uses-permission android:name="android.permission.WRITE_SMS" />
    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.DOWNLOAD_WITHOUT_NOTIFICATION" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.REQUEST_INSTALL_PACKAGES" />
    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS"></uses-permission>

    <application
        android:name=".MyApplication"
        android:icon="@mipmap/sport_map_icon"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/sport_map_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Contorller.DIYActivity06"></activity>
        <activity android:name=".Contorller.SelectDialActivity06" />
        <activity
            android:name=".Contorller.UpdateFirmwareActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Contorller.DIYActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Contorller.SelectDialActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Contorller.AnimalHeatActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Contorller.FeedbackActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Contorller.PrivacyActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Contorller.ShareActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Contorller.SportTrackActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Contorller.SportDataListActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Contorller.CameraActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Contorller.main.MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Contorller.welcome.WelcomeActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Contorller.LoginActivity"
            android:screenOrientation="portrait"
            android:theme="@style/DefaultCityPickerTheme" />
        <activity
            android:name=".Contorller.ForgetPasswordActivity"
            android:screenOrientation="portrait"
            android:theme="@style/DefaultCityPickerTheme" />
        <activity
            android:name=".Contorller.RegisterActivity"
            android:screenOrientation="portrait"
            android:theme="@style/DefaultCityPickerTheme" />
        <activity
            android:name=".Contorller.UserInfoActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Contorller.SetPasswordActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Contorller.UnitSelectActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Contorller.StepReportActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Contorller.SleepReportActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Contorller.HeartReportActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Contorller.BloodPressureReportActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Contorller.BloodOxygenReportActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Contorller.BluetoochCallActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Contorller.AboutActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Contorller.EcgListActivity"
            android:screenOrientation="portrait" />

        <service
            android:name=".Notification.MyNotificationReceiver"
            android:label="@string/app_name"
            android:permission="android.permission.BIND_NOTIFICATION_LISTENER_SERVICE">
            <intent-filter>
                <action android:name="android.service.notification.NotificationListenerService" />
                <action android:name="android.deletion.notification" />
            </intent-filter>
        </service>
        <service android:name=".Service.MainService" />
        <service
            android:name=".Notification.NotificationReceiver"
            android:permission="android.permission.BIND_ACCESSIBILITY_SERVICE">
            <intent-filter>
                <action android:name="android.accessibilityservice.AccessibilityService" />
            </intent-filter>

            <meta-data
                android:name="android.accessibilityservice"
                android:resource="@xml/accessibility_service_config" />
        </service>

        <activity
            android:name="com.yalantis.ucrop.UCropActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity
            android:name=".Contorller.SeachingActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Contorller.EcgDataActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".Contorller.NotificationAppListActivity"
            android:screenOrientation="portrait" />

        <provider
            android:name="androidx.core.content.FileProvider"
            android:authorities="${applicationId}.fileprovider"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/filepaths"
                tools:replace="android:resource" />
        </provider>
    </application>

</manifest>